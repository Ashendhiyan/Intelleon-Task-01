            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="8.0" layoutY="227.0" mnemonicParsing="false" onAction="#btnCategoryOnAction" prefHeight="46.0" prefWidth="211.0" styleClass="btn_suppliers" stylesheets="@style/style.css" text="Category" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
