               <children>
                  <AnchorPane fx:id="side_anchorpane" prefHeight="600.0" prefWidth="250.0" styleClass="side_anchorpane" stylesheets="@style/style.css" />
               </children>
            </VBox>
            <AnchorPane fx:id="context" prefHeight="600.0" prefWidth="787.0" style="-fx-background-color: #b2bec3;" />
         </children>
      </HBox>
      <Button layoutX="7.0" layoutY="88.0" mnemonicParsing="false" onAction="#btnSuppliersOnAction" prefHeight="46.0" prefWidth="211.0" styleClass="btn_suppliers" stylesheets="@style/style.css" text="Suppliers" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="8.0" layoutY="158.0" mnemonicParsing="false" onAction="#btnUnitOnAction" prefHeight="46.0" prefWidth="211.0" styleClass="btn_suppliers" stylesheets="@style/style.css" text="Unit" textFill="#fffefe">
